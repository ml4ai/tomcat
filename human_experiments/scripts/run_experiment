#!/bin/bash

tmux_session_name="processAggregator"


# Kill session with the same name if it already exists
tmux has-session -t $tmux_session_name 2>/dev/null
if [ $? -eq 0 ]
then
tmux kill-session -t $tmux_session_name
fi

# Create new tmux session and detach it
tmux new-session -s $tmux_session_name
